{
  "title": "Q key press",
  "rules": [
    {
      "source": {
        "action": {
          "key": "VK_Q",
          "transition": "Down"
        }
      },
      "target": {
        "actions": [
          {
            "key": "VK_X",
            "transition": "Down"
          }
        ]
      }
    },
    {
      "source": {
        "action": {
          "key": "VK_Q",
          "transition": "Up"
        }
      },
      "target": {
        "actions": [
          {
            "key": "VK_X",
            "transition": "Up"
          }
        ]
      }
    }
  ]
}